{
  "name": "gatsby-mdx-tts",
  "version": "0.0.12",
  "description": "Adds speech output to your Gatsby site using Amazon Polly.",
  "homepage": "https://github.com/flogy/gatsby-mdx-tts",
  "bugs": {
    "url": "https://github.com/flogy/gatsby-mdx-tts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flogy/gatsby-mdx-tts.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc -b",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "preversion": "npm run test",
    "version": "auto-changelog --breaking-pattern \"BREAKING CHANGE:\" -p && git add CHANGELOG.md",
    "postversion": "git push origin master --tags && npm publish",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "author": "Florian Gyger <info@floriangyger.ch>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "tts",
    "text-to-speech",
    "aws",
    "polly",
    "mdx",
    "remark",
    "markdown"
  ],
  "dependencies": {
    "acorn": "^7.1.1",
    "acorn-jsx": "^5.2.0",
    "aws-sdk": "^2.654.0",
    "isomorphic-fetch": "^2.2.1",
    "unist-util-find-after": "^3.0.0",
    "unist-util-find-all-between": "^2.0.0",
    "unist-util-visit": "^2.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "auto-changelog": "^1.16.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gatsby": "^2.20.12",
    "jest": "^25.2.7",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remark": "^11.0.2",
    "remark-mdx": "^1.5.8",
    "to-vfile": "^6.1.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0",
    "gatsby": ">=2.19.0",
    "gatsby-plugin-mdx": ">=1.0.74"
  }
}
